{
  "//": "Disable sorting because JSDoc doesn't sort mutators and protected (public must be first) members correctly",
  "opts": {
    "template": "../node_modules/docdash",
    "verbose": true
  },
  "templates": {
    "default": {
      "outputSourceFiles": false
    },
    "sort": false,
    "outputSourceFiles": false
  },
  "docdash": {
    "static": true,
    "sort": false,
    "search": true,
    "meta": {
      "title": "electron-builder"
    }
  },
  "plugins": ["plugins/markdown"]
}